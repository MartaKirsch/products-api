// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  driver = "better-sqlite3"
  url      = "file:./products.db"
}

model Product {
  id           String @id @default(uuid())
  name         String
  price        Float
  updateDate   DateTime
}